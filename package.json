{
  "name": "personal-blog",
  "version": "1.8.0",
  "private": false,
  "author": "Candido Sales Gomes <candidosg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint": "npx next lint",
    "imageoptim": "npm run imageoptim:gif && npm run imageoptim:jpg && npm run imageoptim:png",
    "imageoptim:gif": "imageoptim \"public/**/*.gif\"",
    "imageoptim:jpg": "imageoptim \"public/**/*.jpg\"",
    "imageoptim:png": "imageoptim --no-imageoptim --imagealpha \"public/**/*.png\""
  },
  "lint-staged": {
    "*.{html,js,json,md,mdx,mjs,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.gif": [
      "imageoptim",
      "git add"
    ],
    "*.jpg": [
      "imageoptim",
      "git add"
    ],
    "*.png": [
      "imageoptim --no-imageoptim --imagealpha",
      "git add"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/questrial": "^4.5.11",
    "framer-motion": "^10.12.8",
    "next": "^13.4.1",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.1",
    "@types/gtag.js": "0.0.12",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
